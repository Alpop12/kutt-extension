{
	"extends":["onepass"],
	"rules": {
		"no-console": 0,
		"no-extend-native": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".jsx", "tsx"] }],
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4
			}
		]
	}
}
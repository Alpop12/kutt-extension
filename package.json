    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^1.4.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-fix-style-only-entries": "^0.3.1",
    "write-webpack-plugin": "^1.1.0",
    "zip-webpack-plugin": "^3.0.0"
  }

{
  "name": "kutturl-extension",
  "version": "1.1.0",
  "main": "background.js",
  "scripts": {
    "dev-chrome": "webpack --watch --mode=development --output-path=chrome",
    "dev-firefox": "webpack --watch --mode=development --output-path=firefox",
    "dev-opera": "webpack --watch --mode=development --output-path=opera",
    "build-chrome": "webpack --mode=production --output-path=chrome",
    "build-firefox": "webpack --mode=production --output-path=firefox",
    "build-opera": "webpack --mode=production --output-path=opera",
    "build-all": "webpack --mode=production --output-path=chrome && webpack --mode=production --output-path=firefox && webpack --mode=production --output-path=opera"
  },
  "author": "abhijithvijayan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
